library(readxl)

cnae_classes <- read_excel('C:/Users/Dell/Desktop/USP_Municipios/dicionario_classes_cnae.xlsx',
                       sheet = 1)

cnae_classes <- cnae_classes %>% 
  rename(codigo = codigo_classe,
         classe_cnae = nome_classe) %>% 
  mutate(codigo = str_pad(codigo, width = 5, side = 'left', pad = '0'))

cnae_classes %>% View()
use_data(cnae_classes)
