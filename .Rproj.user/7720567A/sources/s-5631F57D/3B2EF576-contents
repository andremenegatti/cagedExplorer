regioes_sp <- regioes_sp


regioes_sp <- regioes_sp %>% 
  mutate(regiao_administrativa = str_remove(regiao_administrativa, 'Região Administrativa de '),
         regiao_governo = str_remove(regiao_governo, 'Região de Governo de '),
         regiao_metropolitana = str_remove(regiao_metropolitana, 'Região Metropolitana de '),
         aglomeracao_urbana = str_remove(aglomeracao_urbana, 'Aglomeração Urbana de '))

regioes_sp

devtools::use_data(regioes_sp)
library(devtools)
use_data(regioes_sp, overwrite = TRUE)
